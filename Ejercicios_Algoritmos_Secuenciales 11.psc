Algoritmo Ejercicios_Algoritmos_Secuenciales
	Definir j Como Entero;
	Definir p Como Entero;
	Definir suma como entero;
	j=0;
	p=0;
	Escribir "Ingrese porfavor el primer valor";
	Leer j;
	Escribir "Ingrese porfavor el segundo valor";
	Leer p;
	suma=j+p
	Escribir j, " + ", p, " = ", suma;
	
	
	
FinAlgoritmo
